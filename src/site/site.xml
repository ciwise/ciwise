<?xml version="1.0" encoding="ISO-8859-1"?>

<project name="${project.artifactId}" xmlns="http://maven.apache.org/DECORATION/1.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd">
  
<skin>
    <groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
    <version>1.5</version>
  </skin>
  
  <poweredBy>
        <logo name="Mr. Owl" href="http://www.ciwise.com" img="/img/logo.png"/>
  </poweredBy>

  <custom>
    <fluidoSkin>
      <copyrightClass>pull-right</copyrightClass>
      <gitHub>
        <projectId>ciwise/ciwise</projectId>
        <ribbonOrientation>right</ribbonOrientation>
        <ribbonColor>black</ribbonColor>
      </gitHub>
      <topBarEnabled>false</topBarEnabled>
      <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
      <facebookLike/>
      <googlePlusOne/>
      <twitter>
        <user>dlwhitehurst</user>
        <showUser>true</showUser>
        <showFollowers>true</showFollowers>
      </twitter>
    </fluidoSkin>
  </custom>
      
  <body>
    <menu name="Documentation">
      <item name="Home" href="/index.html"/>
      <item name="Readme" href="README.html" />
    </menu>

    <menu name="Modules" ref="modules" inherit="none"/>
    <menu name="Reports" ref="reports" inherit="none"/>
    <menu name="Links" inherit="none" inheritAsRef="true">
      <item name="Company Site" href="http://www.ciwise.com/"/>
      <item name="Github Homepage" href="http://www.github.com/ciwise"/>
      <item name="LinkedIn Profile" href="https://www.linkedin.com/in/dlwhitehurst"/>
    </menu>
  </body>
  

</project>
